loadstring(game:HttpGet("https://raw.githubusercontent.com/iarotcod/slappybattling/refs/heads/main/funny%20slapple",true))()
loadstring(game:HttpGet('https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/refs/heads/main/SnowFarm'))()

local function serverHop()
    while true do
        local serverList = {}
        pcall(function()
            local response = game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
            for _,v in ipairs(response.data) do
                if v.playing and v.maxPlayers>v.playing and v.id~=game.JobId then
                    table.insert(serverList,v.id)
                end
            end
        end)
        
        if #serverList>0 then
            pcall(function()
                game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId,serverList[math.random(1,#serverList)])
            end)
        end
    end
end

serverHop()
